#include <iostream>
#include <iomanip>
#include <string>
#include <bits/stdc++.h>
#include <algorithm>
#include <cmath>
#define ll long long


using namespace std;



int main() {
ll int t,i=1;
cin>>t;
while(t--) {
ll int num,N, n=0;
cin>>N;
num = N;
while(num>0) {
  n = (n*10) + (num%10);
num/=10;
}
cout<<"Case "<<i<<": ";
if(N==n) {
  cout<<"Yes"<<'\n';
} else {
  cout<<"No"<<'\n';
}
i++;
}
 return 0;
 }
