 #include <iostream>
#include <iomanip>
#include <string>
#include <bits/stdc++.h>
#include <algorithm>
#include <cmath>
#define ll long long


using namespace std;



int main() {
int n, i;
cin>>n;
while(n--) {
  cin>>i;
  if(i==1) {
    cout<<"HARD"<<'\n';
    return 0;
  }
}
cout<<"EASY"<<'\n';
 return 0;
 }
